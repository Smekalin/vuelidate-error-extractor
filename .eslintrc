{
  "root": true,
  "extends": [
    "plugin:vue-libs/recommended"
  ],
  "env": {
    "mocha": true
  },
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxBOF": 1 }]
  },
  "globals": {
    "expect": true,
    "assert": true
  }
}
